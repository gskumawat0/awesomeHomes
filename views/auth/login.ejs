<% include ../partials/header %>
<div class="container width">
 <h1 class="text-center p-5 m-5"> Login To Your Account</h1>
 <div class ="container-fluid">
    <form action = "/login" method="post">
     <div class="form-group"><input type = "text" name = "username" placeholder = "username" class= "form-control">
     </div>
     <div class="form-group"><input type = "password" name = "password" placeholder = "password" class= "form-control">
     </div>
     <div class="form-group">
       <input type = "submit" class= "form-control form-roup btn btn-success">
     </div>
       <p class='m-0 p-0'>Need a account? <a href="/signup">Signup here</a></p>
       <p class='p-0'>Forget password? <a href="/reset-password">Reset here</a></p>
     <input type='hidden' name='_csrf' value='<%= csrfToken %>' >
</form>
 </div>
</div>
<% include ../partials/footer %>